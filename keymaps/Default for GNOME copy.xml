<keymap version="1" name="Default for GNOME copy" parent="Default for GNOME">
  <action id="Resume">
    <keyboard-shortcut first-keystroke="f9" />
    <keyboard-shortcut first-keystroke="shift alt end" />
  </action>
  <action id="StepInto">
    <keyboard-shortcut first-keystroke="f7" />
    <keyboard-shortcut first-keystroke="shift alt page_down" />
  </action>
  <action id="StepOut">
    <keyboard-shortcut first-keystroke="shift f8" />
    <keyboard-shortcut first-keystroke="shift alt page_up" />
  </action>
  <action id="StepOver">
    <keyboard-shortcut first-keystroke="f8" />
    <keyboard-shortcut first-keystroke="shift alt home" />
  </action>
</keymap>